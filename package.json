{
  "name": "static-analysis-a11y-lab",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev --open",
    "lint:ox": "oxlint",
    "lint:es": "eslint",
    "lint:bi": "biome lint",
    "lint": "pnpm run lint:ox && pnpm run lint:es && pnpm run lint:bi",
    "a11y:prepare": "pnpm run build && pnpm run preview",
    "a11y": "axe http://localhost:3000",
    "preview": "rsbuild preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "audit": "python3 .a11y-audit/a11y-auditor/scripts/audit.py src/",
    "audit:watch": "nodemon --watch src --ext tsx,jsx --exec 'npm run audit'",
    "audit:json": "python3 .a11y-audit/a11y-auditor/scripts/audit.py src/ --json audit-report.json",
    "audit:component": "python3 .a11y-audit/a11y-auditor/scripts/audit.py"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intl": "7.1.14",
    "react-router": "7.9.6",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@axe-core/cli": "4.11.0",
    "@biomejs/biome": "2.3.5",
    "@eslint/js": "9.39.0",
    "@rsbuild/core": "1.6.4",
    "@rsbuild/plugin-react": "1.4.2",
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react-webpack5": "^10.0.7",
    "@tailwindcss/postcss": "4.1.17",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-oxlint": "1.28.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-storybook": "^10.0.7",
    "globals": "16.4.0",
    "oxlint": "1.28.0",
    "storybook": "^10.0.7",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
